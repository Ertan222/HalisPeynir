@model Servicee
@{
    ViewData["Title"] = "Info";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-lg-12 mt-4">
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">FEEDS</h5>
            <ul class="feeds">
                <li>
                    ID: @Model.ServiceeID
                </li>
                <li>
                    Product Name: @Model.Name
                </li>
                <li>
                    Product Price: @Model.Price
                </li>


            </ul>

            <div class="button-group">
                <a asp-controller="Servicee" asp-action="Edit" asp-route-id="@Model.ServiceeID" class="btn waves-effect waves-light btn-lg btn-rounded btn-warning">Edit</a>
                <a asp-controller="Servicee" asp-action="Delete" asp-route-id="@Model.ServiceeID" class="btn waves-effect waves-light btn-lg btn-rounded btn-dark">Delete</a>
                <a asp-controller="Servicee" asp-action="List" class="btn waves-effect waves-light btn-lg btn-rounded btn-info">Back to List</a>

            </div>
        </div>
    </div>
</div>

