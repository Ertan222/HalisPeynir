@model Gender
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-lg-12 col-md-12 mt-4">
    <!-- Card -->
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">ID: @Model.GenderID  Name: @Model.GenderName</h4>
            <p class="card-text">Are you sure u wants to delete this gender?</p>

            <div class="button-group">
                <form asp-controller="Gender" asp-action="RealDelete" asp-route-id="@Model.GenderID" method="post">
                    <button type="submit" class="btn waves-effect waves-light btn-lg btn-rounded btn-danger">Delete</button>
                    <a asp-controller="Gender" asp-action="List" asp-route-id="@Model.GenderID" class="btn waves-effect waves-light btn-lg btn-rounded btn-dark">Cancel</a>
                </form>
            </div>
        </div>
    </div>
    <!-- Card -->
</div>

